{
  "multipart": [
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_dummy"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_base"
      },
      "when": {
        "facing": "north"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_base",
        "y": 90
      },
      "when": {
        "facing": "east"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_base",
        "y": 180
      },
      "when": {
        "facing": "south"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_base",
        "y": 270
      },
      "when": {
        "facing": "west"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_clamp"
      },
      "when": {
        "facing": "north",
        "flag": "true"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_clamp",
        "y": 90
      },
      "when": {
        "facing": "east",
        "flag": "true"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_clamp",
        "y": 180
      },
      "when": {
        "facing": "south",
        "flag": "true"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_clamp",
        "y": 270
      },
      "when": {
        "facing": "west",
        "flag": "true"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_top"
      },
      "when": {
        "facing": "north",
        "top": "true"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_top",
        "y": 90
      },
      "when": {
        "facing": "east",
        "top": "true"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_top",
        "y": 180
      },
      "when": {
        "facing": "south",
        "top": "true"
      }
    },
    {
      "apply": {
        "model": "dcs_climate:block/build/metal_ladder_top",
        "y": 270
      },
      "when": {
        "facing": "west",
        "top": "true"
      }
    }
  ]
}